@{
    ViewData["Title"] = "Home Page";
    var slideImage1 = ViewBag.SlideImage1;
    var slideImage2 = ViewBag.SlideImage2;
    var slideImage3 = ViewBag.SlideImage3;
    var SlideHader1= ViewBag.SlideHader1;
    var SlideHader2 = ViewBag.SlideHader2;
    var SlideHader3 = ViewBag.SlideHader3;
    var SlideParagraph1 = ViewBag.SlideParagraph1;
    var SlideParagraph2 = ViewBag.SlideParagraph2;
    var SlideParagraph3 = ViewBag.SlideParagraph3;
    var DescImage1 = ViewBag.DescImage1;
    var DescImage2 = ViewBag.DescImage2;
    var DescImage3 = ViewBag.DescImage3;
    var DescHeader = ViewBag.DescHeader;
    var DescParagraph = ViewBag.DescParagraph;
    var DescPoint1 = ViewBag.DescPoint1;
    var DescPoint2 = ViewBag.DescPoint2;
    var poolImage = ViewBag.PoolImage;
    var PoolHeader = ViewBag.PoolHeader;
    var PoolParagraph = ViewBag.PoolParagraph;
    var RoomHeader = ViewBag.RoomHeader;
    var RoomParagraph = ViewBag.RoomParagraph;
    var RoomImage1 = ViewBag.RoomImage1;
    var RoomImage2 = ViewBag.RoomImage2;
    var RoomImage3 = ViewBag.RoomImage3;
    var RoomPrice1 = ViewBag.RoomPrice1;
    var RoomPrice2 = ViewBag.RoomPrice2;
    var RoomPrice3 = ViewBag.RoomPrice3;
    var RoomImageHeader1 = ViewBag.RoomImageHeader1;
    var RoomImageHeader2 = ViewBag.RoomImageHeader2;
    var RoomImageHeader3 = ViewBag.RoomImageHeader3;
    var RoomImageParagraph1 = ViewBag.RoomImageParagraph1;
    var RoomImageParagraph2 = ViewBag.RoomImageParagraph2;
    var RoomImageParagraph3 = ViewBag.RoomImageParagraph3;
    var ContactHeader = ViewBag.ContactHeader;
    var ContactParagraph = ViewBag.ContactParagraph;
    var ContactAddress = ViewBag.ContactAddress;
    var ContactPhone = ViewBag.ContactPhone;
    var ContactEmail = ViewBag.ContactEmail;
}
<section class="hero-area">
    <div class="hero-slides owl-carousel">

        <!-- Single Hero Slide -->
        <div class="single-hero-slide d-flex align-items-center justify-content-center">
            <!-- Slide Img -->
            <div class="slide-img bg-img" style="background-image: url(@slideImage1);"></div>
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-12 col-lg-9">
                        <!-- Slide Content -->
                        <div class="hero-slides-content" data-animation="fadeInUp" data-delay="100ms">
                            <div class="line" data-animation="fadeInUp" data-delay="300ms"></div>
                            <h2 data-animation="fadeInUp" data-delay="500ms">@SlideHader1</h2>
                            <p data-animation="fadeInUp" data-delay="700ms">@SlideParagraph1</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Single Hero Slide -->
        <div class="single-hero-slide d-flex align-items-center justify-content-center">
            <!-- Slide Img -->
            <div class="slide-img bg-img" style="background-image: url(@slideImage2);"></div>
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-12 col-lg-9">
                        <!-- Slide Content -->
                        <div class="hero-slides-content" data-animation="fadeInUp" data-delay="100ms">
                            <div class="line" data-animation="fadeInUp" data-delay="300ms"></div>
                            <h2 data-animation="fadeInUp" data-delay="500ms">@SlideHader2</h2>
                            <p data-animation="fadeInUp" data-delay="700ms">@SlideParagraph2</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Single Hero Slide -->
        <div class="single-hero-slide d-flex align-items-center justify-content-center">
            <!-- Slide Img -->
            <div class="slide-img bg-img" style="background-image: url(@slideImage3);"></div>
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-12 col-lg-9">
                        <!-- Slide Content -->
                        <div class="hero-slides-content" data-animation="fadeInUp" data-delay="100ms">
                            <div class="line" data-animation="fadeInUp" data-delay="300ms"></div>
                            <h2 data-animation="fadeInUp" data-delay="500ms">@SlideHader3</h2>
                            <p data-animation="fadeInUp" data-delay="700ms">@SlideParagraph3</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</section>
<!-- ##### Hero Area End ##### -->
<!-- ##### Book Now Area Start ##### -->
<div class="book-now-area">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-12 col-lg-10">
                <div class="book-now-form" style="margin-left:100px">
                    <form action="#">
                        <!-- Form Group -->
                        <div class="form-group">
                            <label for="select1">Check In</label>
                            <select class="form-control" id="select1">
                                <option>19 June</option>
                                <option>20 June</option>
                                <option>21 June</option>
                                <option>22 June</option>
                                <option>23 June</option>
                                <option>24 June</option>
                                <option>25 June</option>
                            </select>
                        </div>

                        <!-- Form Group -->
                        <div class="form-group">
                            <label for="select2">Check Out</label>
                            <select class="form-control" id="select2">
                                <option>20 June</option>
                                <option>21 June</option>
                                <option>22 June</option>
                                <option>23 June</option>
                                <option>24 June</option>
                                <option>25 June</option>
                                <option>26 June</option>
                                <option>27 June</option>
                            </select>
                        </div>

                        <!-- Form Group -->
                        <div class="form-group">
                            <label for="select3">Person</label>
                            <select class="form-control" id="select3">
                                <option>02</option>
                                <option>03</option>
                                <option>04</option>
                                <option>05</option>
                                <option>06</option>
                            </select>
                        </div>



                        <!-- Button -->
                       <button type="submit" asp-controller="Logins" asp-action="Login">Book Now</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- ##### Book Now Area End ##### -->
<!-- ##### About Us Area Start ##### -->
<section class="about-us-area">
    <div class="container">
        <div class="row align-items-center">

            <div class="col-12 col-lg-6">
                <div class="about-text text-center mb-100">
                    <div class="section-heading text-center">
                        <div class="line-"></div>
                        <h2>@DescHeader</h2>
                    </div>
                    <p>@DescParagraph</p>
                    <div class="about-key-text">
                        <h6><span class="fa fa-check"></span> @DescPoint1</h6>
                        <h6><span class="fa fa-check"></span> @DescPoint2</h6>
                    </div>
                   
                </div>
            </div>

            <div class="col-12 col-lg-6">
                <div class="about-thumbnail homepage mb-100">
                    <!-- First Img -->
                    <div class="first-img wow fadeInUp" data-wow-delay="100ms">
                        <img src="@Url.Content(DescImage1)" alt="">
                    </div>
                    <!-- Second Img -->
                    <div class="second-img wow fadeInUp" data-wow-delay="300ms">
                        <img src="@Url.Content(DescImage2)" alt="">
                    </div>
                    <!-- Third Img-->
                    <div class="third-img wow fadeInUp" data-wow-delay="500ms">
                        <img src="@Url.Content(DescImage3)" alt="">
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- ##### About Us Area End ##### -->
<!-- ##### Pool Area Start ##### -->
<section class="pool-area section-padding-100 bg-img bg-fixed" style="background-image: url(@poolImage);">
    <div class="container">
        <div class="row justify-content-end">
            <div class="col-12 col-lg-7">
                <div class="pool-content text-center wow fadeInUp" data-wow-delay="300ms">
                    <div class="section-heading text-center white">
                        <div class="line-"></div>
                        <h2>@PoolHeader</h2>
                        <p>@PoolParagraph</p>
                    </div>

                    <div class="row">
                        <div class="col-12 col-sm-4">
                            <div class="pool-feature">
                                <i class="icon-cocktail-1"></i>
                                <p>Pool Beachbar</p>
                            </div>
                        </div>
                        <div class="col-12 col-sm-4">
                            <div class="pool-feature">
                                <i class="icon-swimming-pool"></i>
                                <p>Infinity Pool</p>
                            </div>
                        </div>
                        <div class="col-12 col-sm-4">
                            <div class="pool-feature">
                                <i class="icon-beach"></i>
                                <p>Sunbeds</p>
                            </div>
                        </div>
                    </div>
                    <!-- Button -->
                </div>
            </div>
        </div>
    </div>
</section>
<!-- ##### Pool Area End ##### -->
<!-- ##### Rooms Area Start ##### -->
<section class="rooms-area section-padding-100-0">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-12 col-lg-6">
                <div class="section-heading text-center">
                    <div class="line-"></div>
                    <h2>@RoomHeader</h2>
                    <p>@RoomParagraph</p>
                </div>
            </div>
        </div>

        <div class="row justify-content-center">



            <!-- Single Rooms Area -->
            <div class="col-12 col-md-6 col-lg-4">
                <div class="single-rooms-area wow fadeInUp" data-wow-delay="300ms">
                    <!-- Thumbnail -->
                    <div class="bg-thumbnail bg-img" style="background-image: url(@RoomImage1);"></div>
                    <!-- Price -->
                    <p class="price-from">@RoomPrice1</p>
                    <!-- Rooms Text -->
                    <div class="rooms-text">
                        <div class="line"></div>
                        <h4>@RoomImageHeader1</h4>
                        <p>@RoomImageParagraph1</p>
                    </div>
                    <!-- Book Room -->
                    <a  asp-controller="Logins" asp-action="Login" class="book-room-btn btn palatin-btn">Book Room</a>
                </div>
            </div>

            <!-- Single Rooms Area -->
            <div class="col-12 col-md-6 col-lg-4">
                <div class="single-rooms-area wow fadeInUp" data-wow-delay="100ms">
                    <!-- Thumbnail -->
                    <div class="bg-thumbnail bg-img" style="background-image: url(@RoomImage2);"></div>
                    <!-- Price -->
                    <p class="price-from">@RoomPrice2</p>
                    <!-- Rooms Text -->
                    <div class="rooms-text">
                        <div class="line"></div>
                        <h4>@RoomImageHeader2</h4>
                        <p>@RoomImageParagraph2</p>
                    </div>
                    <!-- Book Room -->
                    <a asp-controller="Logins" asp-action="Login"  class="book-room-btn btn palatin-btn">Book Room</a>
                </div>
            </div>
            <!-- Single Rooms Area -->
            <div class="col-12 col-md-6 col-lg-4">
                <div class="single-rooms-area wow fadeInUp" data-wow-delay="500ms">
                    <!-- Thumbnail -->
                    <div class="bg-thumbnail bg-img" style="background-image: url(@RoomImage3);"></div>
                    <!-- Price -->
                    <p class="price-from">@RoomPrice3</p>
                    <!-- Rooms Text -->
                    <div class="rooms-text">
                        <div class="line"></div>
                        <h4>@RoomImageHeader3</h4>
                        <p>@RoomImageParagraph3</p>
                    </div>
                    <!-- Book Room -->
                    <a asp-controller="Logins" asp-action="Login"  class="book-room-btn btn palatin-btn">Book Room</a>
                </div>
            </div>

        </div>
    </div>
</section>
<!-- ##### Rooms Area End ##### -->
<!-- ##### Contact Area Start ##### -->
