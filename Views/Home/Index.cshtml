            <h2>@ContactHeader</h2>
            <p>@ContactParagraph</p>
        </div>
        <h4 class="wow fadeInUp" data-wow-delay="300ms">@ContactAddress</h4>
        <h5 class="wow fadeInUp" data-wow-delay="400ms">@ContactPhone</h5>
